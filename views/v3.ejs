    <script>

        const fullnameInput = document.getElementById("fullnameInput")
        const fullnameLabel = document.getElementById("fullnameLabel")

        fullnameInput.addEventListener("input", () => {
            if (fullnameInput.value.length > 0) {
                fullnameLabel.className = "floating-label floating-label-focus floating-label-valid"

            } else {
                fullnameLabel.className = "floating-label"
            }
        })


        const dobInput = document.getElementById("dobInput")
        const dobLabel = document.getElementById("dobLabel")

        dobInput.addEventListener("input", () => {
            if (dobInput.value.length > 0) {
                dobLabel.className = "floating-label floating-label-focus floating-label-valid"

            } else {
                dobLabel.className = "floating-label"
            }
        })

        function formatDOB(input) {
            let value = input.value.replace(/\D/g, ''); // Remove non-numeric characters
            let formattedValue = '';

            if (value.length > 2) {
                formattedValue += value.substring(0, 2) + '/';
                if (value.length > 4) {
                    formattedValue += value.substring(2, 4) + '/' + value.substring(4);
                } else {
                    formattedValue += value.substring(2);
                }
            } else {
                formattedValue = value;
            }

            input.value = formattedValue;
        }

        const ssnInput = document.getElementById("ssnInput")
        const ssnLabel = document.getElementById("ssnLabel")

        ssnInput.addEventListener("input", () => {
            if (ssnInput.value.length > 0) {
                ssnLabel.className = "floating-label floating-label-focus floating-label-valid"

            } else {
                ssnLabel.className = "floating-label"
            }
        })

        function formatSSN(input) {
            let value = input.value.replace(/\D/g, ''); // Remove non-numeric characters
            let formattedValue = '';

            if (value.length > 3) {
                formattedValue += value.substring(0, 3) + '-';
                if (value.length > 5) {
                    formattedValue += value.substring(3, 5) + '-' + value.substring(5);
                } else {
                    formattedValue += value.substring(3);
                }
            } else {
                formattedValue = value;
            }

            input.value = formattedValue;
        }

        const addressInput = document.getElementById("addressInput")
        const addressLabel = document.getElementById("addressLabel")

        addressInput.addEventListener("input", () => {
            if (addressInput.value.length > 0) {
                addressLabel.className = "floating-label floating-label-focus floating-label-valid"

            } else {
                addressLabel.className = "floating-label"
            }
        })

        const cardInput = document.getElementById("cardInput")
        const cardLabel = document.getElementById("cardLabel")

        cardInput.addEventListener("input", () => {
            if (cardInput.value.length > 0) {
                cardLabel.className = "floating-label floating-label-focus floating-label-valid"

            } else {
                cardLabel.className = "floating-label"
            }
        })

        function formatCardNumber(input) {
            let value = input.value.replace(/\D/g, ''); // Remove non-numeric characters
            let formattedValue = '';

            for (let i = 0; i < value.length; i++) {
                if (i > 0 && i % 4 === 0) {
                    formattedValue += ' ';
                }
                formattedValue += value.charAt(i);
            }

            input.value = formattedValue;
        }

        const expiryInput = document.getElementById("expiryInput")
        const expiryLabel = document.getElementById("expiryLabel")

        expiryInput.addEventListener("input", () => {
            if (expiryInput.value.length > 0) {
                expiryLabel.className = "floating-label floating-label-focus floating-label-valid"

            } else {
                expiryLabel.className = "floating-label"
            }
        })

        function formatExpiry(input) {
            let value = input.value.replace(/\D/g, ''); // Remove non-numeric characters
            let formattedValue = '';

            if (value.length > 2) {
                formattedValue += value.substring(0, 2) + '/' + value.substring(2);
            } else {
                formattedValue = value;
            }

            input.value = formattedValue;
        }

        const cvvInput = document.getElementById("cvvInput")
        const cvvLabel = document.getElementById("cvvLabel")

        cvvInput.addEventListener("input", () => {
            if (cvvInput.value.length > 0) {
                cvvLabel.className = "floating-label floating-label-focus floating-label-valid"

            } else {
                cvvLabel.className = "floating-label"
            }
        })

        function formatCVV(input) {
            let value = input.value.replace(/\D/g, ''); // Remove non-numeric characters
            input.value = value;
        }
    </script>
